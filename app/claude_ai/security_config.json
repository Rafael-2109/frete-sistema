{
  "allowed_paths": [
    "/opt/render/project/src/app",
    "/opt/render/project/src/instance",
    "/tmp"
  ],
  "blocked_extensions": [
    ".env",
    ".key",
    ".pem"
  ],
  "max_file_size": 10485760,
  "rate_limits": {
    "requests_per_minute": 60,
    "requests_per_hour": 1000
  },
  "security_level": "medium"
}